{
  "browser_action": {
    "browser_style": true,
    "default_title": "List cookies in the active tab",
    "default_popup": "inicio.html",
    "default_icon": "icons/cookie.png"
  },
  "description": "List cookies in the active tab",
  "manifest_version": 2,
  "name": "Detector de Privacidade",
  "version": "1.0",
  "permissions": [ 
    "cookies",
    "contextualIdentities",
    "notifications",
    "storage",
    "webRequest",
    "<all_urls>",
    "tabs"
  ]
}
